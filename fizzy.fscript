open "https://www.fizzyhobbystore.com/card/category/0"

type "$INPUT" ".container form input[type='search']"

click ".container form button.btn2"

waitForNavigation

item ".product"
  .name textOf "h6"
  .link hrefOf "h6 a"
  .set textOf ".small .row"
  .price numbersOf textNodeOf ".card-form p"
  .quantity numbersOf valueOf ".card-form select"
enditem